{
  "name": "download-npm",
  "version": "0.0.1",
  "description": "Download the modules from the NPM registry to HTML5 file system API Edit",
  "main": "dist/index.js",
  "scripts": {
    "start": "budo index.js --live",
    "build": "rm -rf dist && mkdir -p dist && browserify index.js -o dist/downloadNpm.js",
    "test": "babel-tape-runner \"test/**/*.js\" | faucet"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "got": "^6.7.1",
    "semver": "^5.3.0",
    "tape": "^4.6.3",
    "tar-stream": "^1.5.2",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "budo": "^10.0.3",
    "faucet": "0.0.1",
    "testdouble": "^2.1.2"
  },
  "browser": {
    "fs": "./src/fsEs6"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlobo/download-npm.git"
  },
  "keywords": [
    "html5",
    "browser",
    "fs",
    "file",
    "system",
    "FileSystem"
  ],
